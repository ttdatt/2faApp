{
	"$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noExplicitAny": "warn"
			},
			"style": {
				"useTemplate": "off",
				"useSingleVarDeclarator": "off"
			},
			"complexity": {
				"useArrowFunction": "off"
			}
		}
	}
}
